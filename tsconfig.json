{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["DOM", "ES2015"],
    "moduleResolution": "node",
    "module": "ESNext",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "types": ["vite/client"],
    "skipLibCheck": true
  },
  "include": ["src"]
}
